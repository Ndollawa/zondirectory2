{
  "canisters": {
    "backend": {
      "main": "src/backend/main.mo",
      "type": "motoko",
      "dependencies": [
        "payments",
        "zon_pst",
        "CanDBIndex",
        "NacDBIndex"
      ]
    },
    "frontend": {
      "dependencies": [
        "backend"
      ],
      "frontend": {
        "entrypoint": "src/frontend/src/index.html"
      },
      "source": [
        "src/frontend/assets",
        "dist/frontend/"
      ],
      "type": "assets"
    },
    "payments": {
      "main": "src/backend/payments.mo",
      "type": "motoko"
    },
    "zon_pst": {
      "main": "src/zon_pst/lib.mo",
      "type": "motoko"
    },
    "CanDBIndex": {
      "main": "src/storage/CanDBIndex.mo",
      "type": "motoko"
    },
    "NacDBIndex": {
      "main": "src/storage/NacDBIndex.mo",
      "type": "motoko"
    }
  },
  "defaults": {
    "build": {
      "args": "",
      "packtool": "vessel sources"
    }
  },
  "output_env_file": ".env",
  "version": 1,
  "networks": {
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral"
    }
  }
}